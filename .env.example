# Live integration test configuration
# Comma-separated proxies (optional), e.g. host1:port1,host2:port2
JOBSCOUT_TEST_PROXIES=

# HTTP request timeout for live tests in milliseconds
JOBSCOUT_TEST_TIMEOUT_MS=20000

# Verbosity: 0=error, 1=warn, 2=info, 3=debug
JOBSCOUT_TEST_VERBOSE=1

# Optional custom user agent for live tests
JOBSCOUT_TEST_USER_AGENT=
